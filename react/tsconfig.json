{
    "compilerOptions": {
        "jsx": "react",
        //"module": "es2015",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "./build/",
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "es5",
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true
    },
    "include": [
        "src/index.tsx"
    ],
    "exclude": [
        "node_modules",
        "build"
    ]
}
